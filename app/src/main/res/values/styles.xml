<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/taskBarGreen</item>
        <item name="colorPrimaryDark">@color/taskBarGreen</item>
        <item name="colorAccent">@color/backgroundGrey</item>
        <!--<item name="colorBackground">@color/backgroundGrey</item>-->
    </style>

    <style name="CheckEngineTheme" parent="@android:style/TextAppearance">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">12dp</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AlertDialogInput">
        <!--<item name="android:absListViewStyle">true</item>-->

    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
